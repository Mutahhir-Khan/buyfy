ent in cart array\r\n    if (exist.cartQuantity > 0) {\r\n      //modifying cartQuantity on id basis\r\n      return existingProducts.map((existingProduct) => {\r\n        if (existingProduct.id === productId) {\r\n          return {\r\n            ...existingProduct,\r\n            cartQuantity: existingProduct.cartQuantity - 1\r\n          }\r\n        }\r\n        //if not found then return the product\r\n        else\r\n          return existingProduct\r\n      })\r\n    }\r\n    //when cartQuantity is zero then remove from cart array\r\n    else if (exist.cartQuantity === 0) {\r\n      console.log('else if')\r\n      return existingProducts.filter(existingProduct => existingProduct.id !== productId);\r\n    }\r\n    // return the previous state\r\n  } else\r\n      return existingProducts\r\n\r\n}\r\n\r\nexport var productDeletionFromCart = (existingProducts, productId) => {\r\n  var exist = existingProducts.find(existingProduct => existingProduct.id === productId)\r\n  if(exist) \r\n    return existingProducts.filter(existingProduct => existingProduct.id !== productId);\r\n  else \r\n      return existingProducts\r\n}","E:\\MUTAHHIR UNI\\W E B I C A M P\\MODULE 3\\react project\\Buyfy\\code\\src\\Redux\\order\\orderActions.js",[],"E:\\MUTAHHIR UNI\\W E B I C A M P\\MODULE 3\\react project\\Buyfy\\code\\src\\Components\\Navbar\\Navbar.jsx",[],"E:\\MUTAHHIR UNI\\W E B I C A M P\\MODULE 3\\react project\\Buyfy\\code\\src\\Components\\Signout\\Signout.jsx",[],"E:\\MUTAHHIR UNI\\W E B I C A M P\\MODULE 3\\react project\\Buyfy\\code\\src\\Components\\SigninForm\\SigninForm.jsx",[],"E:\\MUTAHHIR UNI\\W E B I C A M P\\MODULE 3\\react project\\Buyfy\\code\\src\\Components\\SignupForm\\SignupForm.jsx",[],"E:\\MUTAHHIR UNI\\W E B I C A M P\\MODULE 3\\react project\\Buyfy\\code\\src\\Components\\GoogleSignin\\GoogleSignin.jsx",[],"E:\\MUTAHHIR UNI\\W E B I C A M P\\MODULE 3\\react project\\Buyfy\\code\\src\\Components\\ProductCard\\ProductCard.jsx",["189","190"],"E:\\MUTA